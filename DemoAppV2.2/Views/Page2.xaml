<Page x:Class="DemoAppV2._2.Views.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DemoAppV2._2.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2" Background="WhiteSmoke" ShowsNavigationUI="False">

    <StackPanel  Margin="30,30,0,0">
        <TextBlock Text="Page 2"/>

        <TextBlock Text="If we click on the Page 1 again in the hamburger menu, it will not navigate us to this page again" Margin="0,20,0,0"/>

        <TextBlock Text="If we did it in the v1.6 one, you can see that it will navigate us back" Margin="0,20,0,0"/>

        <Button Content="Go to child Page" Width="auto" HorizontalAlignment="Left" Margin="0,20,0,0" Click="Button_Click"></Button>
    </StackPanel>
</Page>
